/* GameBoard.java */

package player;

public class GameBoard {
    DList whiteChips;
    DList blackChips;
    Chip[][] chips;

    public GameBoard() {
        whiteChips = new DList();
        blackChips = new DList();
        chips = new Chip[8][8];
        for (int j = 0; j < 8; j++) {
            for (int i = 0; i < 8; i++) {
                chips[i][j] = new Chip(i, j);
            }
        }
    }

}
